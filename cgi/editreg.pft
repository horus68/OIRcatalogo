'function chkfield()
{
   var vArray = document.getElementsByTagName("input");
   var flag=true;
   for(var i=0 ;i<vArray.length;i++) { 
       var tipo=document.getElementById(vArray[i].name).type; 
	   if (tipo !="hidden" && tipo != "button" && tipo != "submit") 
